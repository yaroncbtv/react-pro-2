',justifyContent:'center'}}>\r\n       <div style={{marginTop:'50px'}} class=\"shadow-lg p-3 mb-5  rounded\">\r\n       <h1 style={{color:'white'}}>Login</h1>\r\n       <Form style={{color:'white'}}>\r\n        <Form.Group controlId=\"formBasicEmail\">\r\n            <Form.Label>Email address</Form.Label>\r\n            <Form.Control value={this.state.email} onChange={this.handleChangeEmail} type=\"email\" placeholder=\"Enter email\" />\r\n            <Form.Text className=\"text-muted\">\r\n            We'll never share your email with anyone else.\r\n            </Form.Text>\r\n        </Form.Group>\r\n\r\n        <Form.Group controlId=\"formBasicPassword\">\r\n            <Form.Label>Password</Form.Label>\r\n            <Form.Control value={this.state.password} onChange={this.handleChangePassword} type=\"password\" placeholder=\"Password\" />\r\n        </Form.Group>\r\n        \r\n        <Button onClick={this.handleSubmit} action=\"/\" variant=\"primary\" type=\"submit\">\r\n            Login\r\n        </Button>\r\n        \r\n        </Form>\r\n\r\n        <Nav defaultActiveKey=\"/login\" as=\"ul\">\r\n        <Nav.Item as=\"li\">\r\n            <Nav.Link href=\"/createuser\">New here ? Click to Registered</Nav.Link>\r\n        </Nav.Item>\r\n        </Nav>\r\n        {successLogin}\r\n        </div>\r\n        </div>\r\n        </Container>\r\n        \r\n      </>\r\n    );\r\n  }\r\n  \r\n  \r\n}\r\n\r\nexport default Login;","C:\\Users\\Gael Cohen\\react-project-2-yaroncbtv\\react-pro-2\\src\\reportWebVitals.js",[],"C:\\Users\\Gael Cohen\\react-project-2-yaroncbtv\\react-pro-2\\src\\App.js",["50","51"],"C:\\Users\\Gael Cohen\\react-project-2-yaroncbtv\\react-pro-2\\src\\Components\\CreateUser.jsx",["52","53","54"],"import React from 'react';\r\nimport Form from 'react-bootstrap/Form';\r\nimport Button from 'react-bootstrap/Button';\r\nimport Container from 'react-bootstrap/Container';\r\nimport Alert from 'react-bootstrap/Alert';\r\nimport Nav from 'react-